{
  "version": 2,
  "builds": [
    {
      "src": "public/build/**",
      "use": "@vercel/static"
    },
    {
      "src": "public/index.php",
      "use": "@vercel/php@0.6.0"
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.php"
    }
  ]
}
